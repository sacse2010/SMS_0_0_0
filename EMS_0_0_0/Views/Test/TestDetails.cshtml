@Scripts.Render("~/Scripts/EMS/Test/TestDetails.js")


<div class="rightDiv k-block">
    <h2 class="k-header centerAlign">Test Details</h2>
    <div id="TestDetailsDiv1" class="test">
        <ul>
            <li>
                <label for="TestCode" class="lbl widthSize20_per">Test Code:</label>
                <input type="text" id="txtTestCode" name="TestCode" maxlength="4" class="k-textbox" placeholder="Test Code" required validationmessage="Please enter valid Test Code"/>
            </li>
            <li>
                <label for="TestName" class="lbl widthSize20_per">Test Name:</label>
                <input type="text" id="txtTestName" name="TestName" maxlength="100" pattern="[a-zA-Z0-9 ]+" class="k-textbox" placeholder="Test Name" required validationmessage="Please enter valid Test Name"/>
            </li>
            <li>
                <label for="TestDescription" class="lbl widthSize20_per">Test Description:</label>
                <textarea type="address" id="txtTestDescription" maxlength="2000" name="TestDescription" style="width: 17.4em" class="k-textbox"></textarea>
            </li>

            <li>
                <label for="TestComboBox" class="lbl widthSize20_per">Test Type:</label>
                <input type="text" id="cmbTestType" name="TestComboBox" style="width: 17.4em"/>
            </li>
            <li>
                <label for="TestDropdown" class="lbl widthSize20_per">Test Type2:</label>
                <input type="text" id="ddlTestType" name="TestDropdown" style="width: 17.4em"/>
            </li>
            <li>
                <label for="IsActive" class="lbl widthSize40_per">Is Active:</label>
                <input type="checkbox" id="chkIsActive" name="IsActive" class="k-checkbox"/>

            </li>
            <li class="accept">
                <button id="btnSave" class="k-button" type="submit">Submit</button>
            </li>
            <li class="status"></li>
        </ul>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <div class="panel with-nav-tabs panel-default">
            <div class="panel-heading">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#tab1default" data-toggle="tab">Default 1</a></li>
                    <li><a href="#tab2default" data-toggle="tab">Default 2</a></li>
                    <li><a href="#tab3default" data-toggle="tab">Default 3</a></li>
                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown">Dropdown <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#tab4default" data-toggle="tab">Default 4</a></li>
                            <li><a href="#tab5default" data-toggle="tab">Default 5</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
            <div class="panel-body">
                <div class="tab-content">
                    <div class="tab-pane fade in active" id="tab1default">Default 1</div>
                    <div class="tab-pane fade" id="tab2default">Default 2</div>
                    <div class="tab-pane fade" id="tab3default">Default 3</div>
                    <div class="tab-pane fade" id="tab4default">Default 4</div>
                    <div class="tab-pane fade" id="tab5default">Default 5</div>
                </div>
            </div>
        </div>
    </div>
</div>

<style type="text/css">
    .test ul {
        text-decoration: none;
        list-style: none;


    }

    .test li {
        padding: 5px 5px 5px 5px;
    }

    .lbl {
        width: 20%;
        height: 10%
    }
    .panel.with-nav-tabs .panel-heading{
    padding: 5px 5px 0 5px;
}
.panel.with-nav-tabs .nav-tabs{
	border-bottom: none;
}
.panel.with-nav-tabs .nav-justified{
	margin-bottom: -1px;
}
</style>
<script type="text/javascript">
    $(document).ready(function () {
        $('#tabs').tab();
    });
</script>
